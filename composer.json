{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.0",
		"rhumsaa/uuid": "~2.8@dev",
		"buttercup/protects": "dev-master",
		"mitchellvanw/laravel-doctrine": "dev-master",
		"adamgoose/laravel-annotations": "~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "2.1.x-dev as 2.1.0-RC1",
	  	"benconstable/phpspec-laravel": "dev-laravel-5"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"FinerThings\\": "app/",
		  	"spec\\FinerThings\\": "spec/"
		}
	},
	"autoload-dev": {
		"classmap": [

		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
